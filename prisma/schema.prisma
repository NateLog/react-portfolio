generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model incomes {
  id            Int     @id @default(autoincrement())
  countrycode  String  @db.Char(3)
  year         Int    @db.SmallInt
  incomepercap Decimal @db.Decimal(10, 1)
}
